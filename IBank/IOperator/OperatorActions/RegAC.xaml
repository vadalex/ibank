<UserControl x:Class="IOperator.OperatorActions.RegAC"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      
		xmlns:fm="clr-namespace:UIControlLibrary.Forms;assembly=UIControlLibrary"
		xmlns:my="clr-namespace:UIControlLibrary.SearchEngines;assembly=UIControlLibrary"
		xmlns:my2="clr-namespace:UIControlLibrary.Components;assembly=UIControlLibrary"
		x:Name="This"
		xmlns:ph="clr-namespace:IOperator.Path"
	>

	<StackPanel>
		<TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10">Регистрация карт-счета</TextBlock>
		
		<my:FindAB x:Name="UIFindAB"/>

		<fm:CardAccountFormControl x:Name="UICardAccountForm"/>

		<Button Name="UIButtonRegAC" Content="Зарегистрировать карт счет" Click="UIButtonRegAC_Click"/>
		
		<my2:Message x:Name="UIMessage"/>

		<ph:ToRoot/>

	</StackPanel>

</UserControl>

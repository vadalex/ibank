<UserControl x:Class="IOperator.OperatorActions.RegBankAccount"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 
				xmlns:fm="clr-namespace:UIControlLibrary.Forms;assembly=UIControlLibrary"
				xmlns:my="clr-namespace:UIControlLibrary.SearchEngines;assembly=UIControlLibrary"
				xmlns:my2="clr-namespace:UIControlLibrary.Components;assembly=UIControlLibrary"
				x:Name="This"
				xmlns:ph="clr-namespace:IOperator.Path"			 
			 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	
    <StackPanel>

		<TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10">Регистрация банк-счета</TextBlock>

		<my:FindClient x:Name="UIFindClient"/>

		<fm:BankAccounFormControl x:Name="UIBankAccountForm"/>
		
		<Button Name="UIButtonRegBankAccount" Content="Зарегистрировать банк-счет" Click="UIButtonRegBankAccount_Click"/>

		<my2:Message x:Name="UIMessage"/>

		<ph:ToRoot/>

	</StackPanel>
</UserControl>

<UserControl x:Class="IOperator.OperatorActions.LockUnlockAccessCardClieteControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 
			 
			xmlns:my="clr-namespace:UIControlLibrary.SearchEngines;assembly=UIControlLibrary"
			 xmlns:my2="clr-namespace:UIControlLibrary.Components;assembly=UIControlLibrary"
             xmlns:ph="clr-namespace:IOperator.Path"			 
			 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<StackPanel>
		<TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10">Блокировка/разблокировка учетной записи клиента</TextBlock>

		<my:FindClient x:Name="UIFindClient"/>
		<Button x:Name="UIButtonLock" Click="UIButtonLock_Click">Заблокировать/разблокировать учетную запись</Button>
		<my2:Message x:Name="UIMessage"/>

		<ph:ToRoot/>

	</StackPanel>
</UserControl>
